  "description": "Frontend for cpdb v2",
  "main": "index.js",
  "license": "UNLICENSED",
  "scripts": {
    "build": "CPDB_APP_ENV=prod webpack",
    "start": "webpack-dev-server",
    "test": "CPDB_APP_ENV=test NODE_PATH=src/js mochify --viewport-height 2000 --viewport-width 2000 --require ./test/setup.js --recursive test --transform [ babelify --presets [ es2015 react ] ] --transform envify --https-server 3000 --allow-chrome-as-root",
    "test-debug": "yarn run test --debug",
    "test-watch": "yarn run test --watch",
    "cover": "yarn run test -- --plugin [ mochify-istanbul --instrumenter babel-istanbul --report text --exclude **/mock-api/** --exclude **/utils/vendors.js ] --timeout 10000",
    "cover-html": "yarn run test -- --plugin [ mochify-istanbul --instrumenter babel-istanbul --report html --exclude **/mock-api/** --exclude **/utils/vendors.js ] --timeout 10000",
    "cover-watch": "yarn run test -- --watch --plugin [ mochify-istanbul --instrumenter babel-istanbul --report text --exclude **/mock-api/** --exclude **/utils/vendors.js ] --timeout 10000",
    "cover-circle": "yarn run test -- --plugin [ mochify-istanbul --instrumenter babel-istanbul --report lcovonly --exclude **/mock-api/** --exclude **/utils/vendors.js ] --timeout 10000",
    "live-test": "CPDB_APP_ENV=test wdio wdio.conf.js",
    "lint": "eslint ./src ./test"
  },
