  "scripts": {
    "build": "gulp build",
    "start": "NODE_PATH=src/js budo src/js/index.js:dist/bundle.js --pushstate --host 0.0.0.0 --live -- --transform [ babelify --presets [ es2015 react ] ]",
    "test": "NODE_PATH=src/js mochify --require ./test/setup.js --recursive test --transform [ babelify --presets [ es2015 react ] ]",
    "test-debug": "npm run test -- --debug",
    "test-watch": "npm run test -- --watch",
    "cover": "npm run test -- --plugin [ mochify-istanbul --report text ]",
    "cover-human-readable": "npm run test -- --plugin [ mochify-istanbul --report text ]",
    "cover-circle": "npm run test -- --plugin [ mochify-istanbul --report lcovonly ]",
    "live-test": "nightwatch",
    "lint": "eslint ./src ./test"
  },
